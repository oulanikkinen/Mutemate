{
    "name": "Mutemate",
    "manifest_version": 2,
    "version": "1.0.0",
    "icons": {
        "128": "128.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "background": {
        "persistent": true,
        "scripts": ["background.js"]
    },
    "permissions": ["tabs", "storage"]
}